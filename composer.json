{
    "name": "oxygencms/menus",
    "description": "Oxygen CMS menus package.",
    "license": "MIT",
    "authors": [
        {
            "name": "Pavel Pavlov",
            "email": "pavlovp@icloud.com"
        },
        {
            "name": "Martin Blagoev",
            "email": "mb.reppair@gmail.com"
        }
    ],
    "require": {
        "php": "7.0 - 7.3",
        "spatie/laravel-menu": "^3.4",
        "oxygencms/core": "^1.0"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Oxygencms\\Menus\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Oxygencms\\Menus\\MenuServiceProvider"
            ],
            "aliases": {
                "Menu": "Oxygencms\\Menus\\Facades\\Menu"
            }
        }
    }
}
